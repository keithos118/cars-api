{
   "openapi":"3.0.0",
   "info":{
      "description":"Python Flask API",
      "version":"1.0.0",
      "title":"Python Flask API"
   },
   "tags":[
      {
         "name":"Car Request",
         "description":"API for requesting and return car requests"
      }
   ],
   "paths":{
      "/cars":{
         "get":{
            "tags":[
               "Car Request"
            ],
            "summary":"Returns all Cars",
            "responses":{
               "200":{
                  "description":"OK"
               }
            }
         }
      },
      "/cars/{id}": {
         "parameters": [
            {
               "name": "id",
               "in": "path",
               "required": true,
               "description": "ID of the car that we want to match",
               "type": "string"
            }
         ],
         "get": {
            "tags": [
               "Car Request"
            ],
            "summary": "Get car request with given ID",
            "parameters": [
               {
                  "in": "path",
                  "name": "id",
                  "required": true,
                  "description": "Car Request id",
                  "schema": {
                     "$ref": "#/components/schemas/id"
                  }
               }
            ],
            "responses": {
               "200": {
                  "description": "OK"
               },
               "400": {
                  "description": "Failed. Misunderstood Request."
               },
               "404": {
                  "description": "Failed. Car request not found."
               }
            }
         }
      },
      "/cars/average/{makeModelPrice}":{
         "parameters":[
            {
               "name":"make_model_price",
               "in":"path",
               "required":true,
               "description":"Returns the average price by Make or Model or Year",
               "type":"string"
            }
         ],
         "get":{
            "tags":[
               "Car Request"
            ],
            "summary":"Returns the average price by Make or Model or Year",
            "parameters":[
               {
                  "in":"path",
                  "name":"make_model_price",
                  "required":true,
                  "description":"Car Request name",
                 "schema": {
                            "$ref": "#/components/schemas/name"
                            }
               }
            ],
            "responses":{
               "200":{
                  "description":"OK"
               },
               "400":{
                  "description":"Failed. Misunderstood Request."
               },
               "404":{
                  "description":"Failed. Car request not found."
               }
            }
         }
      }
   },
   "components": {
      "schemas": {
         "id": {
            "properties": {
               "uuid": {
                  "type": "string"
               }
            }
         },
         "makeModelPrice":{
            "properties":{
               "makeModelPrice":{
                  "type":"string"
               }
            }
         }
      }
   }
}